package com.cg.corejava.flowandexception;

public class Exercise2 {

	public void getNthFibonacciNonRec(int n) {
		if(n==1) 
			System.out.print(1+" ");
		else if(n==2) 
			System.out.println(1+" ");
		else {
			int a=1;
			int b=1;
			int count=3;
			while(count<=n) {
				int c=a+b;
				System.out.println();
			}
		}
	}
	
}
