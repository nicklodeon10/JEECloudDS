package com.cg.corejava.ui;

import com.cg.corejava.arrays.Exercise1;
import com.cg.corejava.arrays.Exercise2;

public class MyApplication {

	public static void main(String[] args) {
		
		int[] ar1= {213,123,54,3,212,12,5456,354,87,987};
		System.out.println(new Exercise1().getSecondSmallest(ar1));

		String[] ar2= {"I","am","on","a","Highway","to","Hell"};
		ar2=new Exercise2().getSortedStringWithCaseMod(ar2);
		for(int i=0; i<ar2.length; i++) {
			System.out.print(ar2[i]+" ");
		}System.out.println();
	}

}
