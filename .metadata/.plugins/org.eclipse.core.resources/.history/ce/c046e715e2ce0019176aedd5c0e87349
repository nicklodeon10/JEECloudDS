package com.cg.demomap.dto;

public class Department<T,K> {
	
	T deptId;
	K deptName;
	
	public Department() {
	}
	
	public Department(T deptId, K deptName) {
		this.deptId = deptId;
		this.deptName = deptName;
	}
	
	public T getDeptId() {
		return deptId;
	}
	
	public void setDeptId(T deptId) {
		this.deptId = deptId;
	}
	
	public K getDeptName() {
		return deptName;
	}
	
	public void setDeptName(K deptName) {
		this.deptName = deptName;
	}
	
}
