package com.cg.corejava.flowandexception;

public class Exercise2 {

	public int getNthFibonacciNonRec(int n) {
		String out="";
		if(n==1) 
			out+=" ";
		else if(n==2) 
			out+=" ";
		else {
			int a=1;
			out+=" ";
			int b=1;
			out+=" ";
			int count=3;
			int c;
			while(count<=n) {
				c=a+b;
				out=out+c+" ";
				count++;
				a=b;
				b=c;
			}
		}return c;
	}
	
	public int getNthFibonacciRec(int n) {
	    if (n <= 1) 
	        return n; 
	     return getNthFibonacciRec(n-1) + getNthFibonacciRec(n-2);
	}
	
}
