package com.cg.corejava.arrays;

public class Exercise4 {
	
	public void getCount(char[] ar) {
		int index[]=new int[26];
		for(int i=0; i<26; i++) {
			index[i]=index[ar[i]-97]++;
		}
		for(int i=0; i<26; i++) {
			if(index[i]!=0)
				System.out.println("Index Of "+(char)(65+i)+" is "+index[i]);
		}
	}
	
}
