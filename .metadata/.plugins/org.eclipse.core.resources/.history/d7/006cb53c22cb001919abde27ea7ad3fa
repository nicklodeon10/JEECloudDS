package com.cg.corejava.flowandexception;

public class Exercise3 {

	public void printPrimes(int n) {
		for(int i=1; i<=n; i++) {
			if(isPrime(i))
				System.out.println(i);
		}
	}
	
	public boolean isPrime(int n) {
		if(n==1) return false;
		else if(n==2) return true;
		else {
			for(int i=3; i<=Math.sqrt(n); i++) {
				if(n%2==0)
					
			}
		}
	}
	
}
